import csv

def csv_to_list(file_path):
    header = []
    value_rows = []

    with open(file_path, 'r', newline='') as csvfile:
        csv_reader = csv.reader(csvfile)
        header = next(csv_reader)
        for row in csv_reader:
            value_rows.append(row)
    
    return header, value_rows

    
    

