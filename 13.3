import ipaddress

def get_ip_version(ip_address_str):
    ip = None
    version = None

    try:
        # Convert the string IP address to an IP object
        ip = ipaddress.ip_address(ip_address_str)
        # Get the version of the IP object
        version = ip.version
    except ValueError:
        # Handle cases where the input string is not a valid IP address
        print(f"Error: '{ip_address_str}' is not a valid IP address.")
        ip = None
        version = None
    return ip, version


